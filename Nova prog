int S_FE = ; // Sensor frontal esquerda
int S_FC = ; // Sensor frontal centro
int S_FD = ; // Sensor frontal direita
int S_TE1 = ; //Sensor traseiro  esquerda
int S_TEE = ; //Sensor traseiro extremo esquerda
int S_TC = ; // Sensor traseiro centro
int S_TD1 = ; //Sensor traseiro direita
int S_TED = ; //Sensor traseiro extrema direita
byte mfd = ; // Motor frontal direita
byte mfe = ; // Motor frontal esquerda
byte mtd = ; // Motor traseiro direita
byte mte = ; // Motor traseiro esquerda



void frente()
{
  analogWrite(mfd, 200);
  analogWrite(mtd, 0);
  analogWrite(mofe, 200);
  analogWrite(mte, 0);
}

void direita()
{
  analogWrite(mfd 0);
  analogWrite(mtd, 200);
  analogWrite(mfe, 200);
  analogWrite(mte, 0);
}

void esquerda()
{
  analogWrite(mfd, 200);
  analogWrite(mtd, 0);
  analogWrite(mfe, 0);
  analogWrite(mte, 200);
}

void atualiza()
{
  sensorEsq = analogRead(A2);
  sensorDir = analogRead(A1);
  sensorDir2 = analogRead(A3);
  sensorEsq2 = analogRead(A0);
  sensorVD = analogRead(A5);
  sensorVE = analogRead(A4); 
}


void setup() {

pinMode(mfd, OUTPUT);
pinMode(mfe, OUTPUT);
pinMode(mtd, OUTPUT);
pinMode(mte, OUTPUT);
pinMode(S_FE, INPUT);
pinMode(S_FC, INPUT);
pinMode(S_FD, INPUT);
pinMode(S_TE1, INPUT);
pinMode(S_TEE, INPUT);
pinMode(S_TC, INPUT);
pinMode(S_TD1, INPUT);
pinMode(S_TED, INPUT);


}

void loop() {

,



}
